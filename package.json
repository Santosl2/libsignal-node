{
  "name": "@whiskeysockets/libsignal-node",
  "version": "2.0.1",
  "description": "Open Whisper Systems' libsignal for Node.js",
  "repository": "WhiskeySockets/libsignal-node",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "keywords": [
    "signal",
    "whispersystems",
    "crypto"
  ],
  "license": "GPL-3.0",
  "dependencies": {
    "@bufbuild/protobuf": "^2.6.2",
    "@noble/ciphers": "^1.3.0",
    "@noble/hashes": "^1.8.0",
    "async-mutex": "^0.5.0",
    "curve25519-js": "^0.0.4",
    "protobufjs": "6.8.8"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
