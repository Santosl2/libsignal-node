    "build": "tsup src/index.ts --format=esm --dts",
    "lint": "eslint . --ext .ts"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.55.1",
    "@bufbuild/protoc-gen-es": "^2.6.2",
    "@types/node": "^22.16.5",
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "eslint": "6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
